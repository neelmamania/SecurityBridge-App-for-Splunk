{
    "meta": {
        "apiVersion": "1.0.0"
    },
    "tokens": [
        "__settings__.logging.loglevel",
        "__settings__.additional_parameters.username",
        "__settings__.additional_parameters.password",
        "rest_api_endpoint",
        "global_account.username",
        "global_account.password"
    ],
    "global_settings": {
        "logging": {
            "level": "{{__settings__.logging.loglevel}}"
        }
    },
    "requests": [
        {
            "pre_process": {
                "skip_conditions": [],
                "pipeline": []
            },
            "request": {
                "url": "https://abex3.securitybridge.work/sap/opu/odata/ABEX/EVENTS_SRV/events?$filter=qFrom%20eq%20datetime%272022-11-25T16:07:00%27%20and%20qTo%20eq%20datetime%272022-11-25T17:57:00%27&$format=json",
                "method": "GET",
                "headers": {},
                "auth": {
                    "type": "basic_auth",
                    "options": {
                        "username": "{{global_account.username}}",
                        "password": "{{global_account.password}}"
                    }
                }
            },
            "post_process": {
                "skip_conditions": [
                    {
                        "input": [
                            "{{__response__.body}}",
                            "$"
                        ],
                        "method": "json_empty"
                    }
                ],
                "pipeline": [
                    {
                        "input": [
                            "{{__response__.body}}",
                            "",
                            "{{index}}",
                            "{{host}}",
                            "{{source}}",
                            "{{sourcetype}}"
                        ],
                        "method": "splunk_xml",
                        "output": "__stdout__"
                    },
                    {
                        "input": [
                            "{{__stdout__}}"
                        ],
                        "method": "std_output"
                    }
                ]
            },
            "iteration_mode": {
                "iteration_count": "1",
                "stop_conditions": []
            }
        }
    ]
}